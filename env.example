# Appwrite Configuration
PROJECT_ID=your_project_id_here
API_KEY=your_api_key_here
DATABASE_ID=your_database_id_here

# Collection IDs
PATIENT_COLLECTION_ID=your_patient_collection_id_here
APPOINTMENT_COLLECTION_ID=your_appointment_collection_id_here
PRACTICES_COLLECTION_ID=your_practices_collection_id_here

# Optional Admin Collections
ADMINS_COLLECTION_ID=your_admins_collection_id_here
ADMIN_INVITES_COLLECTION_ID=your_admin_invites_collection_id_here

# Appwrite Endpoint
NEXT_PUBLIC_ENDPOINT=https://cloud.appwrite.io/v1
# OR if using self-hosted:
# NEXT_PUBLIC_ENDPOINT=https://your-appwrite-instance.com/v1

# Storage
NEXT_PUBLIC_BUCKET_ID=your_bucket_id_here

# Admin Configuration
NEXT_PUBLIC_ADMIN_PASSKEY=your_admin_passkey_here
JWT_SECRET=your_super_secret_jwt_key_here_at_least_32_characters

# Application URLs
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# External Services (Optional)
# Twilio for SMS
TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_token
TWILIO_PHONE_NUMBER=your_twilio_phone

# Resend for Email
RESEND_API_KEY=your_resend_api_key

# Stripe for Payments
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key

# Sentry for Error Tracking
SENTRY_DSN=your_sentry_dsn
